{
  "name": "frontend-data-monitoring",
  "version": "0.8.0",
  "private": true,
  "description": "The front-end for BNB, a site that uses linked data to empower everyone in Flanders to consult the decisions made by their local authorities.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-data-monitoring"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:analyze-bundle": "ANALYZE_BUNDLE=true EMBROIDER_CONCAT_STATS=true ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "dev": "ember serve --environment=development --proxy=https://data-monitoring.andres-dev.s.redhost.be/ --secure-proxy=false --port=4202",
    "dev:proxy": "ember serve --environment=development --proxy=https://dev.lokaalbeslist.lblod.info/",
    "qa:proxy": "ember serve --environment=development --proxy=https://lokaalbeslist.lblod.info/",
    "start": "ember serve --environment=development",
    "prod": "ember serve --environment=production",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "release": "release-it"
  },
